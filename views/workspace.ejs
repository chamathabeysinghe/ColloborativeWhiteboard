<!--code for fabric js-->
<script>
    var canvas = new fabric.Canvas('canvas');
</script>

<!--code for actions in js-->
<script>

    //add element to canvas
    function AddElement(x,y,type){

        if(type=='add-text'){
            canvas.add(new fabric.IText('Tap and Type', {
                fontFamily: 'arial',
                fontSize:14,
                left: x,
                top: y ,
            }));
        }
        if(type=='add-square'){
            canvas.add( new fabric.Rect({
                left: x,
                top: y,
                fill:'',
                stroke:'red',
                strokeWidth:1,
                width: 100,
                height: 100
            }));
        }
        if(type=='add-squaref'){
            canvas.add( new fabric.Rect({
                left: x,
                top: y,
                fill:'red',
                stroke:'red',
                strokeWidth:1,
                width: 100,
                height: 100
            }));
        }
        if(type=='add-circle'){
            canvas.add( new fabric.Circle({
                left: x,
                top: y,
                radius: 100,
                stroke: 'red',
                strokeWidth: 1,
                fill: ''
            }));
        }
        if(type=='add-circlef'){
            canvas.add( new fabric.Circle({
                left: x,
                top: y,
                radius: 100,
                stroke: 'red',
                strokeWidth: 1,
                fill: 'red'
            }));
        }
    }
    //for button drag
    function drag(ev){
        ev.dataTransfer.setData('type',ev.target.id);
        console.log("Dragging");
    }
    //for drop button
    function drop(ev) {

        console.log("Dropped    "+ev.dataTransfer.getData('type'));
        console.log(ev.clientX+"  "+ev.clientY);
        AddElement(ev.clientX-300,ev.clientY-100,ev.dataTransfer.getData('type'));
        sendMessage();
    }

    function allowDrop(ev) {
        ev.preventDefault();
    }

//    delete items
    document.onkeydown = function (e) {
        e = e || window.event;
        // use e.keyCode
        if(e.keyCode==46){
            canvas.getActiveObject().remove();
            sendMessage();
        }
    };



    canvas.on('text:changed', function(e) {
        //this will only work using the dev build
        sendMessage();
    });
    canvas.on('object:modified', function(e) {
        //do something, maybe count the number of characters
        sendMessage();
    });

    canvas.on("text:editing:exited", function (e) {
        console.log('updated text:',e.target.text);
        console.log("text:editing:exited");
        sendUserWorkData(e.target.text)
    });



    var socket = io();
    socket.on('receive message', function(msg){
        console.log("Receive a message oooi");
        loadCanvas(msg);
    });
    //send canvas change
    function sendMessage() {
        var json = JSON.stringify( canvas.toJSON() );

        console.log(json);
        socket.emit('draw message',json);

    }
    //receive canvas changes
    function loadCanvas(json) {

        // parse the data into the canvas
        canvas.loadFromJSON(json);

        // re-render the canvas
        canvas.renderAll();

        // optional
        canvas.calculateOffset();
    }

//send messages about workspace content
    function sendUserWorkData(text){
        console.log("Send a AI message");
        socket.emit('ai message',text);
    }
//  send chat messages
    function sendChatMessage(text){
        socket.emit('chat message',text);
    }

//    receive chat messages
    socket.on('chat message',function (msg) {
        console.log('receive message :'+msg);
        appendMessage(msg);
//        appendTips(msg);
    })


//    receive ai helper messages
    socket.on('receive ai',function (msg) {
       console.log('Received the message');
        var entityArray=msg[0];
        var searchResultArray=msg[1];
        for (i = 0; i < entityArray.length; i++) {
            appendTips(entityArray[i],searchResultArray[i]);
        }

    });


//    append message to chat box
    function appendMessage(text){
        var htmlText='<br><div class="direct-chat-msg doted-border">'+
                +'<div class="direct-chat-info clearfix"><span class="direct-chat-name pull-left">Osahan</span></div>'+
                '<img alt="message user image" src="http://bootsnipp.com/img/avatars/bcf1c0d13e5500875fdd5a7e8ad9752ee16e7462.jpg" class="direct-chat-img">'+
                '<div class="direct-chat-text">'+text+'</div><div class="direct-chat-info clearfix"><span class="direct-chat-timestamp pull-right">3.36 PM</span>'+
                '</div> </div>';
        $('#site-message-box').html='<p>asdfadfad</p>';


        var currentHtml=$('#div2').html();
        $('#div2').html(currentHtml+htmlText);

    }

//    append content to support area
    function appendTips(text,content) {
        console.log("Appending to Modal");
        console.log(text);
        console.log(content);
        var currentHtml=$('#ai_tips').html();
        var appendHtml='<p>Want to know about ' +
                '<a href="#" class="" data-toggle="modal" data-target="#myModal" data-content="'+content+'" data-title='+text+'>'+text+'</a>' +
                '</p>';
        $('#ai_tips').html(currentHtml+appendHtml);
    }

</script>

